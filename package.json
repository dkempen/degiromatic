{
  "name": "degiromatic",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./dist/src/main.js",
    "build": "tsc",
    "build:dev": "tsc --sourceMap",
    "dev": "tsc-watch --sourceMap --onSuccess \"node dist/src/main.js\"",
    "docker": "docker compose -f compose.dev.yaml up --build",
    "lint": "eslint --max-warnings 0",
    "bundle": "rollup -c bundle/rollup.config.ts"
  },
  "dependencies": {
    "croner": "^9.1.0",
    "degiro-api": "git://github.com/CasperVerswijvelt/degiro-api.git#60aaf6678a9aaca1a20d990555943b3d31d0721f",
    "dotenv": "^17.2.1",
    "pino": "^9.9.0",
    "pino-pretty": "git://github.com/pinojs/pino-pretty.git#99b1566c9961a25e586f7e9ae57fdc8438203d6e",
    "speakeasy": "^2.0.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.3.0",
    "@types/speakeasy": "^2.0.10",
    "eslint": "^9.33.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "jiti": "^2.5.1",
    "rollup": "^4.47.1",
    "tsc-watch": "^7.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "overrides": {
    "node-fetch": "^2.7.0"
  }
}
